{
    "name": "synergy/pagebuilder",
    "description": "Generate widgetized pages",
    "license": "BSD-3-Clause",
    "keywords": [
        "page builder",
        "jqgrid",
        "zf3",
        "module",
        "widgets"
    ],
    "homepage": "https://github.com/odiaseo/pagebuilder",
    "authors": [
        {
            "name": "Pele Odiase",
            "email": "peleodiase@yahoo.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "synergy/common": "*",
        "synergy/synergydatagrid": "*"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "*",
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-0": {
            "PageBuilderTest": "tests/module/"
        },
        "psr-4": {
            "PageBuilder\\": "src/PageBuilder"
        },
        "classmap": [
            "./Module.php",
            "./tests/module/PageBuilderTest/Module.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "laminas/laminas-component-installer": true,
            "laminas/laminas-dependency-plugin": true
        }
    }
}
